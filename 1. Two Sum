// Time - O(n^2)

class Solution {
public:
    vector<int> twoSum(vector<int>& nums, int target) {
        int len_nums = nums.size();
        vector<int> ans;
        for(int i = 0; i < len_nums; i++)
        {
            for(int j = 0; j < len_nums; j++)
            {
                if(i == j)
                    continue;
                if(nums[i] + nums[j] == target)
                {
                    ans.push_back(i);
                    ans.push_back(j);
                    return ans;
                }
            }
        }
        return ans;
    }
};
